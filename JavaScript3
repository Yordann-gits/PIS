function isEven(n) {
  return typeof n === 'number' && n % 2 === 0;
}

function divisibleBy5And9(n) {
  return typeof n === 'number' && n % 5 === 0 && n % 9 === 0;
}

function maxOfTwo(a, b) {
  if (a === b) return `равни са (${a})`;
  return a > b ? a : b;
}

console.log(isEven(4));             
console.log(isEven(7));             
console.log(divisibleBy5And9(45)); 
